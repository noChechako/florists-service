{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "radix": false,
    "trailing-comma": [
      false
    ],
    "no-console": false,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "always"
    ]
  },
  "rulesDirectory": []
}
